{
  "name": "darts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx dotenv -e .env.development.local -- next dev",
    "build": "npx dotenv -e .env.development.local -- bash -c 'prisma generate && next build'",
    "start": "next start",
    "lint": "next lint",
    "studio": "npx dotenv -e .env.development.local -- npx prisma studio",
    "studio:test": "npx dotenv -e .env.test -- npx prisma studio",
    "test": "npx dotenv -e .env.test -- jest",
    "test:watch": "npx dotenv -e .env.test -- jest --watch",
    "test:integration": "JEST_ENV=integration npx dotenv -e .env.test -- jest --runInBand",
    "test:integration:full": "sh ./run-integration-tests.sh",
    "migrate": "npx dotenv -e .env.development.local -- npx prisma db push",
    "migrate:test": "npx dotenv -e .env.test -- npx prisma db push",
    "delete:tournament": "npx dotenv -e .env.development.local -- ts-node -P tsconfig.script.json scripts/delete-tournament.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@vercel/analytics": "^1.1.1",
    "@vercel/postgres": "^0.7.2",
    "@vercel/speed-insights": "^1.0.2",
    "axios": "^1.6.7",
    "jest-mock-extended": "^4.0.0",
    "next": "^14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prisma": "^5.15.1",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5",
    "vercel": "^32.3.0"
  }
}
